{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "ES2021",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,
    "strictNullChecks": false,
    "noImplicitAny": false,
    "strictBindCallApply": false,
    "forceConsistentCasingInFileNames": false,
    "noFallthroughCasesInSwitch": false,
    "paths": {
      "@common/*": ["src/common/*"],
      "@database/*": ["src/database/*"],
      "@libs/*": ["src/libs/*"],
      "@utils/*": ["src/utils/*"],
      "@salary/*": ["src/salary/*"],
      "@deliveryMan/*": ["src/delivery-man/*"],
      "@generalFeatures/*": ["src/general-features/*"],
      "@auth/*": ["src/auth/*"],
      "@users/*": ["src/users/*"],
      "@upload/*": ["src/upload/*"],
      "@integrations/*": ["src/integrations/*"],
      "@sms/*": ["src/sms/*"],
      "@shortLink/*": ["src/short-link/*"],
      "@json-store/*": ["src/json-store/*"],
      "@shared/*": ["src/shared/*"],
      "@token/*": ["src/token/*"],
      "@scarper/*": ["src/scraper/*"],
      "@monthly-income/*": ["src/monthly-incomes/*"],
      "@company/*": ["src/company/*"],
      "@cors/*": ["src/cors/*"],
      "@scraper/*": ["src/scraper/*"],
      "@occasion/*": ["src/occasion/*"],
      "@statistics/*": ["src/statistics/*"],
      "@transaction/*": ["src/transaction/*"]
    }
  }
}
